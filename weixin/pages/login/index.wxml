<view class="page__bd">
  <image style="width:100%;margin-bottom:20px" src="../../imgs/login-bg.png"></image>
  <!--<view style="text-align:center;margin-bottom:10px;">登录</view>-->
  <wux-cell-group>
    <wux-cell hover-class="none">
      <wux-input placeholder='请输入用户名' maxlength='25' bind:change="bindUserName" >
        <image style="width:20px;height:20px;margin-right:20px;" src="../../imgs/username.png"></image>
      </wux-input> 
    </wux-cell> 
    <wux-cell hover-class="none">
      <wux-input placeholder='请输入密码' password maxlength='25' bind:change="bindPassword">
        <image style="width:20px;height:20px;margin-right:20px;" src="../../imgs/pwd.png"></image>
      </wux-input>
    </wux-cell>
  </wux-cell-group>
